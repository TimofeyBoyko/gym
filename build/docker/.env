#common
NODE_ENV=dev

#database
DB_NAME=gym
DB_USER=gymUser
DB_PASSWORD=gymPassword
DB_HOST=db
DB_PORT=5432
DB_URI=postgres://${DB_USER}:${DB_PASSWORD}@127.0.0.1:${DB_PORT}/${DB_NAME}

#docker
